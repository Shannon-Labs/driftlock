{
  "name": "driftlock",
  "zone_id": "YOUR_ZONE_ID",
  "account_id": "YOUR_ACCOUNT_ID",
  "domain": "driftlock.com",
  "settings": {
    "security_level": "medium",
    "waf": {
      "enabled": true,
      "rules": [
        {
          "id": "617b3f285832410097a970e407d26e70",
          "action": "block",
          "description": "Block SQL injection attempts"
        },
        {
          "id": "4866121627f14b2d9d5ffb8a90a8487e",
          "action": "challenge",
          "description": "Challenge suspicious requests"
        }
      ]
    },
    "ddos_protection": {
      "enabled": true,
      "sensitivity": "high"
    },
    "ssl": {
      "mode": "strict",
      "certificate_status": "active"
    },
    "caching": {
      "level": "aggressive",
      "browser_cache_ttl": 7200,
      "edge_cache_ttl": 86400
    },
    "page_rules": [
      {
        "target": "api.driftlock.com/*",
        "bypass_cache": true,
        "security_level": "high"
      },
      {
        "target": "driftlock.com/*",
        "cache_level": "aggressive",
        "min_tls_version": "1.2"
      }
    ]
  }
}