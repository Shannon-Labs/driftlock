    "@vueuse/core": "^10.7.0",
    "@vueuse/head": "^2.0.0",
    "chart.js": "^4.5.1",
    "firebase": "^12.6.0",
    "highlight.js": "^11.11.1",
    "lucide-vue-next": "^0.303.0",
    "marked": "^17.0.0",
    "pinia": "^3.0.4",
    "vue": "^3.4.0",
    "vue-chartjs": "^5.3.3",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.25.0",
    "firebase-tools": "^13.29.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.9.3",
    "vite": "^5.0.0",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.25"
